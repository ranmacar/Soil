        "nft",
        "defi"
    ],
    "author": "Soil Platform Team",
    "license": "MIT",
    "devDependencies": {
        "@cloudflare/workers-types": "^4.20240529.0",
        "prettier": "^3.2.5",
        "typescript": "^5.4.5",
        "vitest": "^1.6.0",
        "wrangler": "^3.57.0"
    },
    "dependencies": {
        "@emurgo/cardano-serialization-lib-browser": "^11.5.0"
    }
}