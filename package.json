{
    "name": "soil-platform-backend",
    "version": "1.0.0",
    "description": "Cloudflare Workers backend for Soil platform - decentralized marketplace for ideas and products",
    "main": "worker.js",
    "scripts": {
        "dev": "wrangler dev",
